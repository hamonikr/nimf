+uid_t
+nimf_get_loginuid (void)
+{
+  gchar *loginuid;
+  static uid_t uid = -1;
+
+  if (uid == (uid_t) -1)
+  {
+    g_file_get_contents ("/proc/self/loginuid", &loginuid, NULL, NULL);
+
+    if (loginuid)
+    {
+      errno = 0;
+      uid = strtol (loginuid, NULL, 10);
+
+      g_free (loginuid);
+
+      if (!errno)
+        return uid;
+    }
+
+    uid = getuid ();
+  }
+
+  return uid;
+}
